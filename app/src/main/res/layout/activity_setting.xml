<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingActivity">

    <!-- 설정화면 -->
    <RelativeLayout
        android:id="@+id/android_lar"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/setting"
        android:clipToOutline="true"
        tools:ignore="MissingConstraints,RedundantNamespace,UnusedAttribute">

        <Button
            android:id="@+id/profilebtn"
            android:layout_width="346dp"
            android:layout_height="111dp"
            android:layout_marginTop="130dp"
            android:layout_marginLeft="32dp"
            android:background="#00ff0000" />

        <Button
            android:id="@+id/noticebtn"
            android:layout_width="165dp"
            android:layout_height="164dp"
            android:layout_marginTop="262dp"
            android:layout_marginLeft="32dp"
            android:background="#00ff0000" />

        <Button
            android:id="@+id/soundbtn"
            android:layout_width="165dp"
            android:layout_height="164dp"
            android:layout_marginTop="262dp"
            android:layout_marginLeft="213dp"
            android:background="#00ff0000" />

        <Button
            android:id="@+id/alarmbtn"
            android:layout_width="165dp"
            android:layout_height="164dp"
            android:layout_marginTop="446dp"
            android:layout_marginLeft="32dp"
            android:background="#00ff0000" />

        <Button
            android:id="@+id/themebtn"
            android:layout_width="165dp"
            android:layout_height="164dp"
            android:layout_marginTop="446dp"
            android:layout_marginLeft="213dp"
            android:background="#00ff0000" />

        <Button
            android:id="@+id/centerbtn"
            android:layout_width="346dp"
            android:layout_height="62dp"
            android:layout_marginTop="632dp"
            android:layout_marginLeft="32dp"
            android:background="#00ff0000" />


    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>