<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/btnStartRoute"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#000000"
        android:clipToOutline="true"
        tools:ignore="MissingConstraints,RedundantNamespace,UnusedAttribute">

        <TextView
            android:id="@+id/textView"
            android:layout_width="212dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="105dp"
            android:layout_marginTop="149dp"
            android:layout_marginEnd="99dp"
            android:background="@drawable/login_fast_login" />

        <ImageButton
            android:id="@+id/kakaobtn"
            android:layout_width="147dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView"
            android:layout_alignParentStart="true"
            android:layout_marginStart="41dp"
            android:layout_marginLeft="41dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/login_kakao" />

        <ImageButton
            android:id="@+id/facebookbtn"
            android:layout_width="147dp"
            android:layout_height="178dp"
            android:layout_below="@+id/textView"
            android:layout_alignParentEnd="true"
            android:layout_marginLeft="209dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="42dp"
            android:background="@drawable/login_facebook" />

        <ImageButton
            android:id="@+id/naverbtn"
            android:layout_width="147dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/kakaobtn"
            android:layout_alignParentStart="true"
            android:layout_marginStart="41dp"
            android:layout_marginLeft="41dp"
            android:layout_marginTop="13dp"
            android:background="@drawable/login_naver" />

        <ImageButton
            android:id="@+id/googlebtn"
            android:layout_width="147dp"
            android:layout_height="178dp"
            android:layout_below="@+id/facebookbtn"
            android:layout_alignParentEnd="true"
            android:layout_marginLeft="209dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="41dp"
            android:background="@drawable/login_google" />

        <ImageButton
            android:id="@+id/guestbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/googlebtn"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="42dp"
            android:layout_marginLeft="42dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="41dp"
            android:layout_marginBottom="145dp"
            android:background="@drawable/login_nouser" />
    </RelativeLayout>


</LinearLayout>